{
    "name": "project/app-laravel",
    "type": "project",
    "description": "Hotel Bookings Web App Manager based on { Blade || Php , HTML , Js , Css } From Scratch .",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.0.2",
        "guzzlehttp/guzzle": "^7.2",
        "laravel/framework": "9.x-dev",
        "laravel/tinker": "2.x-dev",
        "laravel/sanctum": "^2.8",
    },
    "require-dev": {
        "fakerphp/faker": "^1.9.1",
        "laravel/pint": "^1.0",
        "laravel/sail": "^1.18",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^5.4.28",
        "nunomaduro/termwind": "^1.13",
        "phpunit/phpunit": "^9.1",
        "spatie/laravel-ignition": "1.x-dev"

    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Seeders\\": "database/seeders/",
            "Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "files": [
            ""
        ],
        "psr-4": {
            "Illuminate\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-clear-compiled": [
            "@php artisan clear-compiled"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "8.x-dev"
        },
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "source",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    }
}
